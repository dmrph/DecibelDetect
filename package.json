{
  "name": "decibel-recorder-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "start:backend": "uvicorn backend:app --reload --host 0.0.0.0 --port 8000",
    "start:frontend": "npx expo start",
    "start:all": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "dependencies": {
    "expo": "~50.0.13",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.5",
    "expo-av": "~13.10.5",
    "expo-file-system": "~16.0.8",
    "react-native-web": "~0.19.6",
    "react-dom": "18.2.0",
    "@expo/metro-runtime": "~3.1.3",
    "typescript": "^5.3.0",
    "@types/react": "~18.2.45"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "concurrently": "^8.2.0"
  },
  "private": true
}
