{
  "name": "decibel-recorder-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start:backend": "cd Backend && source ../venv/bin/activate && uvicorn backend:app --reload --host 127.0.0.1 --port 8000",
    "start:frontend": "cd Frontend && npx expo start",
    "start:website": "cd Frontend && npm run start",
    "start:all": "concurrently \"npm run start:backend\" \"npm run start:frontend\" \"npm run start:website\""
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@react-navigation/native": "^7.0.0-rc.21",
    "@types/react": "~18.2.45",
    "expo": "~50.0.13",
    "expo-av": "~13.10.5",
    "expo-file-system": "~16.0.8",
    "expo-status-bar": "~1.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "react-native": "^0.73.6",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-web": "~0.19.6",
    "react-navigation-stack": "^2.10.4",
    "react-router-dom": "^6.27.0",
    "react-scripts": "^5.0.1",
    "typescript": "^5.3.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "concurrently": "^8.2.2"
  },
  "private": true,
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
